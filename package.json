{
  "name": "minecraft-perf-viewer",
  "version": "0.0.1",
  "description": "A viewer for minecraft /perf output",
  "productName": "Minecraft /Perf viewer",
  "author": "Christian Fraenkel <fraenkelc@users.noreply.github.com>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "build": "quasar build"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.4",
    "core-js": "^3.21.1",
    "d3": "^7.3.0",
    "jszip": "^3.7.1",
    "plotly.js": "^2.10.1",
    "quasar": "^2.5.5",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@cyclonedx/webpack-plugin": "^1.0.1",
    "@quasar/app": "^3.2.9",
    "@quasar/quasar-app-extension-testing": "^2.0.2",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.8",
    "@types/d3": "^7.1.0",
    "@types/node": "^17.0.21",
    "@types/plotly.js": "^1.54.20",
    "@types/resize-observer-browser": "^0.1.7",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-vue": "^8.5.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "quasar-app-extension-qhierarchy": "^1.0.0-alpha.1"
  },
  "browserslist": [
    "last 10 Chrome versions",
    "last 10 Firefox versions",
    "last 4 Edge versions",
    "last 7 Safari versions",
    "last 8 Android versions",
    "last 8 ChromeAndroid versions",
    "last 8 FirefoxAndroid versions",
    "last 10 iOS versions",
    "last 5 Opera versions"
  ],
  "engines": {
    "node": ">= 12.22.1",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
